{
  "manifest_version": 2,

  "name": "Study or Die",
  "description": "If you dont study you die",
  "version": "2.0",
  
"background": {
  "scripts": ["background.js"]
    
  },
  "content_scripts": [
        {
        "matches": [
            "https://docs.google.com/*"
            ],
        "js": ["content.js"],
        "run_at": "document_start",
        "all_frames": true, 
        "match_about_blank": true      
        }
    
    ],

  
  "permissions": [
    "activeTab", 
  "tabs",
  "storage",
  "downloads"
  ]
}